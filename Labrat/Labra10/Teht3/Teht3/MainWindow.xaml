<Window x:Class="Teht3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Teht3"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Margin="0,0,-6.6,-5.2">
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="452,114,0,0" TextWrapping="Wrap" Text="Ikkunan Leveys (L)" VerticalAlignment="Top" Height="13" Width="128"/>
